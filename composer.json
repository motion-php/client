{
    "name": "motion-php/client",
    "description": "API client written in PHP for Motion",
    "require": {
        "php-http/discovery": "1.x-dev",
        "psr/http-client": "1.0.x-dev",
        "psr/http-client-implementation": "*",
        "psr/http-factory-implementation": "*",
        "psr/http-message": "2.0.x-dev"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "dev-master",
        "guzzlehttp/psr7": "2.6.x-dev",
        "laravel/pint": "dev-main",
        "pestphp/pest": "2.x-dev",
        "pestphp/pest-plugin-mock": "2.x-dev",
        "phpstan/phpstan": "1.11.x-dev",
        "rector/rector": "0.15.x-dev",
        "symfony/var-dumper": "6.3.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MotionPhp\\Client\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adam Paterson",
            "email": "hello@adampaterson.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
